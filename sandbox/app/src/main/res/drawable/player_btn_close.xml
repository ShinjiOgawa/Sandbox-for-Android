<!-- ======================================== YouTube Player Closeボタン定義 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_focused="true"  android:state_pressed="false" android:drawable="@drawable/player_btn_close_pressed" />
  <item android:state_focused="true"  android:state_pressed="true"  android:drawable="@drawable/player_btn_close_pressed" />
  <item android:state_focused="false" android:state_pressed="true"  android:drawable="@drawable/player_btn_close_pressed" />
  <item android:drawable="@drawable/player_btn_close_normal" />
</selector>
